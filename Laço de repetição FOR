package pkgfor;

/**
 *
 * @author aluno
 */
public class For {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        // TODO code application logic here
        
        for(int i = 1; i <= 10; i++){
            System.out.println(i);
        }
        
        for(int i = 10; i >= 1; i++){
            System.out.println(i);
        }
    }
    
}
