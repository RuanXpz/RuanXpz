package pkgfor;

/**
 *
 * @author aluno
 */
public class For {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        // TODO code application logic here
        
        for(int i = 7; i <= 77;i += 7){
            System.out.println(i);
        }
        
        System.out.print("--------------------");
        
        for(int i = 5; i >= 1; i++){
            System.out.println(i);
        }
    }
}
